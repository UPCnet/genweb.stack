<?xml version="1.0"?>
<rolemap>
  <roles>
    <role name="Anonymous"/>
    <role name="Authenticated"/>
    <role name="Contributor"/>
    <role name="Editor"/>
    <role name="Manager"/>
    <role name="Member"/>
    <role name="Owner"/>
    <role name="Reader"/>
    <role name="Reviewer"/>
    <role name="WebMaster"/>
  </roles>
  <permissions>
    <permission name="LogosFooter: Add Logos_Footer"
                acquire="False">
      <role name="Contributor"/>
      <role name="Manager"/>
      <role name="WebMaster"/>
    </permission>
    <permission name="Add Collage content" acquire="True">
      <role name="Contributor"/>
    </permission>
    <permission name="windowZ: Add Window" acquire="True">
      <role name="Contributor"/>
    </permission>
    <permission name="PloneFormGen: Add Content"
                acquire="True">
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="WebMaster"/>
    </permission>
    <permission name="PloneFormGen: Add Custom Scripts"
                acquire="True">
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="WebMaster"/>
    </permission>
    <permission name="PloneFormGen: Add Data Savers"
                acquire="True">
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="WebMaster"/>
    </permission>
    <permission name="PloneFormGen: Add Mailers"
                acquire="True">
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="WebMaster"/>
    </permission>
    <permission name="PloneFormGen: Download Saved Input"
                acquire="True">
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="WebMaster"/>
    </permission>
    <permission name="PloneFormGen: Edit Advanced Fields"
                acquire="True">
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="WebMaster"/>
    </permission>
    <permission name="PloneFormGen: Edit Mail Addresses"
                acquire="True">
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="WebMaster"/>
    </permission>
    <permission name="PloneFormGen: Edit Python Fields"
                acquire="True">
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="WebMaster"/>
    </permission>
    <permission name="PloneFormGen: Edit TALES Fields"
                acquire="True">
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="WebMaster"/>
    </permission>
  </permissions>
</rolemap>
